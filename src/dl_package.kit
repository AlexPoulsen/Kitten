include "limits.h";
include "unistd.h";
include "sys/types.h";
include "sys/stat.h";
import init;


function main() {
    updatePackages();
}

function updatePackages() {
    var cwd: Ptr[Char] = malloc(${PATH_MAX: Int} * sizeof(Char));
    if (getcwd(cwd, ${PATH_MAX: Int} * sizeof(Char)) != null) {
        chdir("/usr/local/lib/kit/kitten/_kitten_json_dir/");
        system("git pull");
        chdir(cwd);
    } else {
        perror("getcwd() error");
    }
}